<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<type>PLAIN</type>
	<title>Simple: control tree browser (Plain HTML / Any browser)</title>
	<abstract>true</abstract>

	<prototype>ctrl-simple-abstract</prototype>

	<charset>UTF-8</charset>
	<default>browse.htm</default>
	
	<renderer>
		<type>ACM.TPL</type>
		<suffix>.tpl</suffix>
	</renderer>
	
	<secure>true</secure>
	<auth>true</auth>
</settings>