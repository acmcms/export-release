<?xml version="1.0" encoding="UTF-8"?>
<settings>
	<type>PLAIN</type>
	<title>Simple: form builder (Plain HTML / Any browser)</title>
	<abstract>true</abstract>

	<prototype>ctrl-simple-execute</prototype>

	<import package="skin-jsclient" namespace="client"/>
	<import package="vlapan-richedit" namespace="richedit"/>

	<default>form.htm</default>
	<charset>UTF-8</charset>
	
	<renderer>
		<type>ACM.TPL</type>
		<suffix>.tpl</suffix>
	</renderer>
	
	<generate>dynamic</generate>
	<secure>true</secure>
	<auth>true</auth>
</settings>