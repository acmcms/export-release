/* <%FINAL: 'text/css' %><%FORMAT: 'css' %><%= '/' + '*' %> */
HTML{ /* ie - absolute positioning ? */
	width:100%;
	height:100%;
	color:black;
	background:white;
}
IMG { /* ie - nicer image resizing */
	-ms-interpolation-mode: bicubic;
}
PRE, CODE, SAMP {
	white-space: pre;
}
PRE{
	background: #FAF8F0;
}
CODE, SAMP, .code {
	display: block;
	padding: 0.5em 1em;
	border: 1px solid #BEBAB0;
	overflow: auto;
	color: block;
	background: #FAF8F0;
}
TD, TH {
	vertical-align: top;
}
/* <%/FORMAT%><%/FINAL%> */